    int v;
    char str1[12000];
    char str2[12000];
    char st[12000];
    for (int i = 0; i <20 ; i++) {
        v =p[i];
        print_star();
        printf("抽到的单词意思为： %s\n",p1[v-1]);
        gets(str1);
        strcpy(str2,p1[v-2]);
        if (strcmp(str1,str2)==0){
            printf("输入的单词正确\n");}
        else
        {
            printf("输入的单词错误\n");
            printf("应该打出的单词为 %s\n",p1[v-2]);
        }
    }
}
